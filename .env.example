# Application Configuration
VITE_APP_NAME=JSON Fields
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# GitHub Repository (for header/stats)
VITE_GITHUB_REPO_OWNER=sciphergfx
VITE_GITHUB_REPO_NAME=json-fields

# API Configuration (if needed in future)
# VITE_API_URL=https://api.example.com

# Maximum file sizes (in bytes)
VITE_MAX_JSON_SIZE=5242880  # 5MB
VITE_MAX_EXPORT_SIZE=10485760  # 10MB

# Performance
VITE_DEBOUNCE_DELAY=300  # milliseconds

# Theme
VITE_DEFAULT_COLOR_MODE=light  # light or dark
